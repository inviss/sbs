<?xml version="1.0" encoding="UTF-8"?><jeus-system xmlns="http://www.tmaxsoft.com/xml/ns/jeus" version="6.0" xmlns:ns2="http://java.sun.com/xml/ns/javaee" xmlns:ns3="http://java.sun.com/xml/ns/persistence">
   <node>
      <name>PDASETST01</name>
      <engine-container>
         <name>container1</name>
         <id>104</id>
         <base-port>10791</base-port>
         <command-option>-Xms256m -Xmx512m -XX:MaxPermSize=128m -Xbootclasspath/p:/app/boapm/BoAPM.WA2/lib/javassist.jar:/app/boapm/BoAPM.WA2/lib/boapm_rt.jar:/app/boapm/BoAPM.WA2/lib/boapm_wa.jar -Dboapm.conf=/app/boapm/BoAPM.WA2/conf/boapm_wa.conf</command-option>
         <engine-command>
            <type>servlet</type>
            <name>engine1</name>
         </engine-command>
         <sequential-start>true</sequential-start>
         <system-logging>
            <name>jeus</name>
            <handler>
               <file-handler>
                  <name>handler1</name>
                  <level>FINE</level>
                  <valid-day>1</valid-day>
                  <buffer-size>0</buffer-size>
               </file-handler>
            </handler>
         </system-logging>
      </engine-container>
      <class-ftp>true</class-ftp>
      <sequential-start>true</sequential-start>
      <enable-webadmin>true</enable-webadmin>
      <system-logging>
         <name>jeus</name>
         <handler>
            <file-handler>
               <name>handler1</name>
               <level>FINE</level>
               <valid-day>1</valid-day>
               <buffer-size>0</buffer-size>
            </file-handler>
         </handler>
      </system-logging>
   </node>
   <resource>
      <data-source>
         <database>
            <vendor>others</vendor>
            <export-name>dasdb</export-name>
            <data-source-class-name>com.ibm.db2.jcc.DB2ConnectionPoolDataSource</data-source-class-name>
            <data-source-type>ConnectionPoolDataSource</data-source-type>
            <database-name>DASDB</database-name>
            <port-number>50000</port-number>
             <server-name>10.150.12.62</server-name>
             <!--<server-name>10.30.23.48</server-name> -->  
            <user>das</user>
            <password>das</password>
            <property>
               <name>DriverType</name>
               <type>java.lang.Integer</type>
               <value>4</value>
            </property>
            <connection-pool>
               <pooling>
                  <min>30</min>
                  <max>50</max>
                  <step>2</step>
                  <period>3600000</period>
               </pooling>
               <wait-free-connection>
                  <enable-wait>true</enable-wait>
                  <wait-time>300000</wait-time>
               </wait-free-connection>
               <check-query>select * from sysibm.sysdummy1</check-query>
               <check-query-period>600000</check-query-period>
            </connection-pool>
         </database>
         <database>
            <vendor>others</vendor>
            <export-name>sbsdev</export-name>
            <data-source-class-name>com.ibm.db2.jcc.DB2ConnectionPoolDataSource</data-source-class-name>
            <data-source-type>ConnectionPoolDataSource</data-source-type>
            <database-name>SBSDB</database-name>
            <port-number>50000</port-number>
            <server-name>10.10.16.46</server-name>
            <user>dat</user>
            <password>dat</password>
            <property>
               <name>DriverType</name>
               <type>java.lang.Integer</type>
               <value>4</value>
            </property>
            <connection-pool>
               <pooling>
                  <min>2</min>
                  <max>50</max>
                  <step>4</step>
                  <period>3600000</period>
               </pooling>
               <wait-free-connection>
                  <enable-wait>true</enable-wait>
                  <wait-time>300000</wait-time>
               </wait-free-connection>
               <check-query>select * from sysibm.sysdummy1</check-query>
               <check-query-period>1800000</check-query-period>
            </connection-pool>
         </database>
      </data-source>
   </resource>
   <application>
      <absolute-path>/app/jeus/jeus6/webhome/app_home</absolute-path>
      <name>DAS</name>
      <path>DAS</path>
      <web-component/>
      <deployment-type>COMPONENT</deployment-type>
      <deployment-target>
         <target>
            <engine-container-name>PDASETST01_container1</engine-container-name>
         </target>
      </deployment-target>
